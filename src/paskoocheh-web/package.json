{
  "name": "paskoocheh-website-web",
  "private": true,
  "scripts": {
    "ci-build": "run-s next-build copy-static-dirs-to-standalone",
    "ci-build-lint-test": "npm-run-all -l -s graphql-codegen -p ci-build lint-test",
    "ci-lint-test": "run-s graphql-codegen lint-test",
    "copy-static-dirs-to-standalone": "cp -r .next/static .next/standalone/.next/static && cp -r public .next/standalone/public",
    "dev": "npm-run-all -l -s package-changed -p graphql-codegen dev-remove-webpack-cache-if-stale -p graphql-codegen-watch dev-next-dev < /dev/null",
    "dev-build": "npm-run-all -l -s package-changed -p graphql-codegen dev-remove-webpack-cache-if-stale -s next-build copy-static-dirs-to-standalone",
    "dev-build-start": "run-s dev-build dev-start",
    "dev-remove-webpack-cache-if-stale": "dotenv -oe .env.local -- node src/scripts/removeWebpackCacheIfStale",
    "dev-lint-fix": "run-s eslint-fix prettier-fix",
    "dev-lint-test": "run-s package-changed graphql-codegen lint-test",
    "dev-next-dev": "PORT=3000 next dev",
    "dev-start": "HOSTNAME=127.0.0.1 PORT=3000 dotenv -oe .env.local -- node .next/standalone/server.js",
    "jest": "jest",
    "playwright": "npm-run-all -l -p test-build playwright-install -s playwright-test",
    "playwright-install": "DEBIAN_FRONTEND=noninteractive playwright install --with-deps chromium",
    "playwright-test": "playwright test",
    "prod-build": "run-s graphql-codegen next-build copy-static-dirs-to-standalone",
    "eslint-check": "eslint . --ext='js,ts,tsx'",
    "eslint-fix": "eslint . --ext='js,ts,tsx' --fix",
    "graphql-codegen": "graphql-codegen",
    "graphql-codegen-watch": "npm run graphql-codegen -- --watch",
    "lint-test": "run-p -l eslint-check jest playwright prettier-check",
    "knip": "knip",
    "next-build": "next build",
    "npm-check-updates": "npm-check-updates",
    "package-changed": "package-changed",
    "postinstall": "patch-package",
    "prettier-check": "prettier --check .",
    "prettier-fix": "prettier --write .",
    "test-build": "run-s test-next-build test-copy-static-dirs-to-standalone",
    "test-copy-static-dirs-to-standalone": "cp -r .next-test/static .next-test/standalone/.next-test/static && cp -r public .next-test/standalone/public",
    "test-dev": "npm-run-all -l -s package-changed graphql-codegen -p graphql-codegen-watch test-next-dev < /dev/null",
    "test-dev-build-start": "run-s package-changed graphql-codegen test-build test-start",
    "test-next-dev": "PORT=3001 dotenv -oe .env.test -- next dev",
    "test-next-build": "dotenv -oe .env.test -- next build",
    "test-start": "HOSTNAME=127.0.0.1 PORT=3001 dotenv -oe .env.test -- node .next-test/standalone/server.js",
    "ts-json-schema-generator": "ts-json-schema-generator --no-type-check --out='src/strings/strings.schema.json' --path='src/types/stringTypes.ts' --tsconfig='tsconfig.json' --type='Strings'",
    "typescript-check": "tsc --noEmit"
  },
  "browser": {
    "src/strings/strings.schema.json": false
  },
  "engines": {
    "node": "20.x",
    "npm": "10.x",
    "yarn": "This project uses NPM (see README.markdown)"
  },
  "dependencies": {
    "@ariakit/react": "^0.4.1",
    "@artsy/fresnel": "^7.1.3",
    "@asl-19/emotion-utils": "^0.6.2",
    "@asl-19/eslint-config": "^0.8.0",
    "@asl-19/js-dom-utils": "^0.2.1",
    "@asl-19/js-utils": "^0.3.3",
    "@asl-19/next-utils": "^0.6.0",
    "@asl-19/react-dom-utils": "^0.1.1",
    "@asl-19/react-loading-progress-indicator": "^0.3.0",
    "@asl-19/use-overflow-state": "^0.1.1",
    "@axe-core/playwright": "^4.8.4",
    "@emotion/react": "^11.11.3",
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/typescript": "^4.0.1",
    "@graphql-codegen/typescript-graphql-request": "^6.2.0",
    "@graphql-codegen/typescript-operations": "^4.0.1",
    "@playwright/test": "^1.41.2",
    "@testing-library/jest-dom": "^6.4.1",
    "@testing-library/react": "^14.2.1",
    "@types/gtag.js": "^0.0.18",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.16",
    "@types/react": "^18.2.51",
    "@types/react-dom": "^18.2.18",
    "@types/web-app-manifest": "^1.0.7",
    "@types/webpack-bundle-analyzer": "^4.6.3",
    "constate": "^3.3.2",
    "csp-header": "^5.2.1",
    "dotenv-cli": "^7.3.0",
    "envalid": "^8.0.0",
    "eslint": "^8.56.0",
    "eslint-plugin-react-memo": "^0.0.3",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "graphql": "^16.8.1",
    "graphql-request": "6.1.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jwt-decode": "^4.0.0",
    "keen-slider": "^6.8.6",
    "match-sorter": "^6.3.3",
    "next": "^14.1.0",
    "node-mocks-http": "^1.14.1",
    "normalize-html-whitespace": "^1.0.0",
    "npm-run-all2": "^6.1.2",
    "patch-package": "^8.0.0",
    "prettier": "^3.2.4",
    "react": "~18.2.0",
    "react-dom": "~18.2.0",
    "react-error-boundary": "^4.0.12",
    "scroll-into-view-if-needed": "^3.1.0",
    "sharp": "^0.33.2",
    "ts-pattern": "^5.0.6",
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "@parcel/watcher": "^2.4.0",
    "knip": "^4.3.0",
    "npm-check-updates": "^16.14.14",
    "package-changed": "^3.0.0",
    "ts-json-schema-generator": "^1.5.0",
    "webpack-bundle-analyzer": "^4.10.1"
  }
}
