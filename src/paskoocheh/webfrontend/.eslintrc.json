{
    "env": {
        "browser": true
    },
    "extends": "eslint:recommended",
    "globals": {
        "ga": true,
        "grecaptcha": true,
        "Flickity": true,
        "paskoocheh": true
    },
    "rules": {
        "indent": [
            "error",
            4,
            {
                "outerIIFEBody": 0
            }
        ],
        "linebreak-style": [
            "error",
            "unix"
        ],
        "no-console": "off",
        "quotes": [
            "error",
            "single"
        ],
        "semi": [
            "error",
            "always"
        ]
    }
}
